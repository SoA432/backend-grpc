    "debug": "^4.1.1",
    "grpc_tools_node_protoc_ts": "^2.5.11",
    "jasmine": "^3.5.0",
    "jasmine-ts": "^0.3.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
